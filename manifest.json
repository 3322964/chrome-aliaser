{
    "manifest_version": 2,
    "name": "Aliaser",
    "description": "Parameterized URL Alias Utility",
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "version": "1.0",
    "omnibox": {"keyword": "a"},
    "permissions": [
      "tabs"
    ],
    "background": {
      "scripts": ["eventPage.js"],
      "persistent": false
    },
    "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
    }
}